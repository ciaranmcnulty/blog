{
    "frontmatter": {
        "date": [
            "date"
        ]
    },
    "baseURL": "https://www.dantleech.com/",
    "languageCode": "en-gb",
    "paginate": 50,
    "title": "Dans Blog",
    "theme": "dantleech",
    "permalinks": {
        "posts": "/blog/:year/:month/:day/:slug/",
        "categories": "/blog/categories/:slug/"
    },
    "module": {
      "mounts": [
         {
            "source": "content",
            "target": "content"
         },
         {
            "source": "static/images",
            "target": "assets/images"
         },
         {
            "source": "static",
            "target": "static"
         },
         {
            "source": "layouts",
            "target": "layouts"
         },
         {
            "source": "data",
            "target": "data"
         },
         {
            "source": "assets",
            "target": "assets"
         },
         {
            "source": "i18n",
            "target": "i18n"
         },
         {
            "source": "archetypes",
            "target": "archetypes"
         }
      ]
   }
}

